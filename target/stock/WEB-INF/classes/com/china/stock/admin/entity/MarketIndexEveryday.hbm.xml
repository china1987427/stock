<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.china.stock.admin.entity.MarketIndexEveryday" table="market_index_everyday">
		<id name="id" type="integer" column="id">
			<generator class="identity" />
		</id>
		<property name="marketName" type="string" column="market_name" />
		<property name="date" type="date" column="date" />
		<property name="numberDate" type="date" column="number_date" />
		<property name="week" type="string" column="week" />
		<property name="riseorfallCase" type="string" column="riseorfall_case" />
		<property name="inTime" type="string" column="in_time" />
		<property name="mOpeningIndex" type="string" column="m_opening_index" />
		<property name="mClosingIndex" type="string" column="m_closing_index" />
		<property name="mRiseorfall" type="float" column="m_riseorfall" />
		<property name="mAmplitude" type="string" column="m_amplitude" />
		<property name="mLowIndex" type="string" column="m_low_index" />
		<property name="mHighIndex" type="string" column="m_high_index" />
		<property name="mTotalhand" type="long" column="m_totalhand" />
		<property name="sum" type="long" column="sum" />
	</class>
</hibernate-mapping>